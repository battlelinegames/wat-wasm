{
  "name": "wat-wasm",
  "version": "1.0.25",
  "description": "WebAssembly Text to binary WASM module tools.  It includes a version of wat2wasm that has optimization flag.  It also has a version of wasm2wat to convert from WASM binary back to WAT.  This tool provides a CLI built on top of WebAssembly Binary Toolkit and binaryen.js.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "wasm-opt": "bin/wasm-opt",
    "watwasm": "bin/wat-wasm",
    "wat-wasm": "bin/wat-wasm",
    "wat2wasm": "bin/wat2wasm",
    "wasm2wat": "bin/wasm2wat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battlelinegames/wat-wasm.git"
  },
  "keywords": [
    "WebAssembly",
    "WebAssembly Text",
    "wat2wasm",
    "wasm2wat",
    "wasmopt",
    "WASM",
    "WAT",
    "Binaryen",
    "WABT",
    "WebAssembly Binary Toolkit",
    "Binaryen.js",
    "wabt.js"
  ],
  "author": "Rick Battagline <rick@battagline.com> (embed.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/battlelinegames/wat-wasm/issues"
  },
  "homepage": "https://embed.com/wasm/wat-wasm.html",
  "dependencies": {
    "binaryen": "^98.0.0",
    "chalk": "^3.0.0",
    "wabt": "^1.0.19"
  }
}
